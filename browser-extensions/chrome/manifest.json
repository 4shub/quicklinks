{
  "name": "Quicklinks for Chrome",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Quickly create quicklinks from your browser",
  "homepage_url": "http://quicklinks.shub.club",
  "permissions": [
    "tabs",
    "*://localhost:1867/*"
  ],
  "browser_action": {
    "default_title": "browser action demo",
    "default_popup": "src/index.html"
  }
}